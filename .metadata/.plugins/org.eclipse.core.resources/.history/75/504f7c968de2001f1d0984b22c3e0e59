package quiz23;

public class Video implements Runnable  {

	@Override
	public void run() {
		for(int i=1; i<=10; i++) {
			
			System.out.println("동영상을 다운로드 합니다");
			
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
		
	}

}
