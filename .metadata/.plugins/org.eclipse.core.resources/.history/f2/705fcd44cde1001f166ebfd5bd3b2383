package ramda.stream;

import java.util.ArrayList;
import java.util.List;
import java.util.Random;

public class MainClass02 {

	public static void main(String[] args) {
		
		List<Integer>list=new ArrayList<>();
		
		for(int i=0; i<100; i++) {
			list.add(new Random().nextInt(100)+1); // 1~100 랜덤 수
		}
		
		System.out.println(list.toString());
		
		System.out.println("-------------------------");
		
		// 중복제거 distinct
		// 정렬 sorted(오름차순)
		
		list.stream().distinct().sorted().forEach(a->System.out.print(a+" "));
		
		System.out.println();
		
		list.stream().distinct().sorted( (o1, o2) -> Integer.compare(o1, o2))
		.forEach(a->System.out.print(a+" ")); // 오름차순
		
		System.out.println();
		
		list.stream().distinct().sorted( (o1, o2) -> Integer.compare(o2, o1))
		.forEach(a->System.out.print(a+" ")); // 내림차순
		
	}
}
