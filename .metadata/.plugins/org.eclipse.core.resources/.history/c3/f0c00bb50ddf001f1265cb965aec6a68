package baeckjoon;

import java.util.Scanner;

public class Problem25206 {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		double r=0;
		double r2=0;
		
		for(int i=0; i<20; i++) {
			double sum=0;
			String str=sc.next(); // 과목 이름
			double score=sc.nextDouble(); // 학점
			String credit=sc.next(); // 등급
			
			switch (credit) {
			case "A+":
				sum=4.5;
				break;
			case "A0":
				sum=4.0;
				break;
			case "B+":
				sum=3.5;
				break;
			case "B0":
				sum=3.0;
				break;
			case "C+":
				sum=2.5;
				break;
			case "C0":
				sum=2.0;
				break;
			case "D+":
				sum=1.5;
				break;
			case "D0":
				sum=1.0;
				break;
			case "F":
				sum=0.0;
				break;

			default:
				if(credit.equals("P")) {
					sum=0;
				}
				break;
			}
			
			r+=(sum*score);
			r2+=score;
		}
		
		System.out.printf("%.6f",r/r2);
	}
}
