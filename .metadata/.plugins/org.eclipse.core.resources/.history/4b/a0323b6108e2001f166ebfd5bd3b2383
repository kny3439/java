package baeckjoon;

import java.util.Scanner;

public class Problem2720 {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		
		double quater=0.25;
		double dime=0.10;
		double nickel=0.05;
		double penny=0.01;
		
		for(int i=0; i<a; i++) {
			
			int num=sc.nextInt();
			System.out.println(num%quater);
		}
	}
}
