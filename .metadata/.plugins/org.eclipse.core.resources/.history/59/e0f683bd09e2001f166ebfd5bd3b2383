package baeckjoon;

import java.util.Scanner;

public class Problem2720 {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		
		double quater=25;
		double dime=10;
		double nickel=0.5;
		double penny=0.1;
		
		for(int i=0; i<a; i++) {
			
			int num=sc.nextInt();
			while(true) {
				System.out.println(num/quater);
				double r=num%quater;
				System.out.println(r/dime);
				double r2=r%dime;
				System.out.println(r2/dime);
				double r3=r2%nickel;
				System.out.println(r3%nickel);
			}
			
		}
	}
}
